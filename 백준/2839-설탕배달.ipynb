   "source": [
    "n = int(input())\n",
    "\n",
    "cache = [10000 for _ in range(5001)]\n",
    "    \n",
    "cache[3] = 1 \n",
    "cache[5] = 1 \n",
    "\n",
    "\n",
    "for idx in range(6, 5001) : \n",
    "    \n",
    "    cache[idx] = min(cache[idx-3], cache[idx-5]) + 1\n",
    "    \n",
    "print(cache[n] if cache[n] < 10000 else -1)"
   ]
