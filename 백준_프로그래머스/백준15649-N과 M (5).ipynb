   "source": [
    "import sys \n",
    "from itertools import permutations \n",
    "\n",
    "n, m = map(int, input().split())\n",
    "lst = list(map(int, input().split()))\n",
    "lst.sort()\n",
    "\n",
    "answer = []\n",
    "for p in permutations(lst, m) : \n",
    "    answer.append(p)\n",
    "    \n",
    "for ans in answer : \n",
    "    for s in ans : \n",
    "        print(s, end = ' ')"
   ]
