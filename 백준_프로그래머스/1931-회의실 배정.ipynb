   "source": [
    "n = int(input()) \n",
    "table = [list(map(int, input().split())) for _ in range(n)]\n",
    "table.sort(key = lambda x: (x[1], x[0]))\n",
    "\n",
    "cnt, end = 0, 0\n",
    "for i in range(n) : \n",
    "    if table[i][0] >= end : \n",
    "        end = table[i][1]\n",
    "        cnt += 1 \n",
    "           \n",
    "print(cnt)"
   ]
