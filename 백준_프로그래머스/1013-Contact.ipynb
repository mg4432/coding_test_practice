   "source": [
    "import re \n",
    "import sys \n",
    "\n",
    "n = int(input())\n",
    "p = re.compile('(100+1+|01)+')\n",
    "\n",
    "for i in range(n) : \n",
    "    sign = str(input().strip())\n",
    "    print(sign)\n",
    "    if p.fullmatch(sign) :\n",
    "        print('YES')\n",
    "    else : \n",
    "        print('NO')"
   ]
